{
  "expo": {
    "slug": "dietary-guide",
    "name": "v1.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-light.png",
    "scheme": "rork-nutrition-companion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/DGlogo-Photoroom.png",
      "dark": {
        "image": "./assets/images/DGlogo-Photoroom-modified.png",
        "backgroundColor": "#181c1f"
      },
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyrusdemon7.dg",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-light.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "rork-nutrition-companion"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.cyrusdemon7.dg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "75b41f90-f8cd-438b-8b5d-9662c2fd7173"
      }
    }
  }
}
