# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# Auth0 Configuration (if still needed for migration)
EXPO_PUBLIC_AUTH0_DOMAIN=your_auth0_domain.auth0.com
EXPO_PUBLIC_AUTH0_CLIENT_ID=your_auth0_client_id

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api

# Development Environment
EXPO_PUBLIC_ENV=development

# AI Service Configuration
# OpenRouter - Primary AI service (supports multiple models including free ones)
# Get your key from: https://openrouter.ai/keys
EXPO_PUBLIC_OPENROUTER_API_KEY=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

# Optional: Choose specific OpenRouter model (defaults to DeepSeek V3)
# Free models: google/gemini-2.0-flash-exp:free, meta-llama/llama-3.1-8b-instruct:free
# Premium models: deepseek/deepseek-v3, google/gemini-2.5-flash, openai/gpt-4o-mini
# EXPO_PUBLIC_OPENROUTER_MODEL=deepseek/deepseek-v3

# OpenAI - Fallback AI service (optional)
# Get your key from: https://platform.openai.com/api-keys
# EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Additional API Keys
# EXPO_PUBLIC_GOOGLE_API_KEY=your_google_api_key
